{
  "name": "micro-sso-router",
  "description": "Microservice for routing requests",
  "version": "1.0.26",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.7.4"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "sso",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-sso-router.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-sso-router/issues"
  },
  "homepage": "https://github.com/telemark/micro-sso-router",
  "now": {
    "alias": [
      "sso.router.t-fk.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "coveralls": "3.0.1",
    "nsp": "3.2.1",
    "standard": "11.0.1",
    "tap": "12.0.0"
  },
  "dependencies": {
    "marked": "0.3.19",
    "micro": "9.3.1",
    "wildcard": "1.1.2"
  }
}